<?xml version="1.0" encoding="ISO-8859-1"?>
<settings>

  <!-- camera settings -->
  <camera1>
    <transpose type="boolean">True</transpose>
  </camera1>

  <!-- film settings -->
  <film>
    <extension type="string" values=",A750,A647,Cy5,Cy3,Cy2,A405,FITC,YFP,GFP,BF"></extension>
  </film>

  <!-- stage settings -->
  <stage>
    <x_sign type="int">-1</x_sign>
    <y_sign type="int">-1</y_sign>
    <flip_axis type="boolean">True</flip_axis>
  </stage>

  <!-- misc control -->
  <misc>
    <filter_names type="string">1,2,3,4,5,6</filter_names>
    <filter_position type="int">0</filter_position>
  </misc>

  <!-- steve settings -->
  <mosaic>
    <flip_horizontal type="boolean">False</flip_horizontal>
    <flip_vertical type="boolean">False</flip_vertical>
    <transpose type="boolean">True</transpose>
    <!-- <obj1 type="string">100x,0.107,0.0,0.0</obj1> -->
    <obj1 type="string">60x,0.178,0.0,0.0</obj1>
    <obj2 type="string">10x,1.070,-49.1,266.0</obj2>
    <obj3 type="string">4x,2.675,96.0,108.0</obj3>
  </mosaic>

</settings>
